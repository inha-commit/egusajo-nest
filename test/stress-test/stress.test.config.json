{
  "config": {
    "target": "http://localhost:3000",
    "phases": [
      {
        "duration": 60,
        "arrivalRate": 5
      }
    ],
    "defaults": {
      "headers": {
        "Content-Type": "application/json"
      }
    },
    "payload": {
      "path": "./tokens.csv",
      "fields": ["token"]
    }
  },
  "scenarios": [
    {
      "flow": [
        {
          "get": {
            "url": "/",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/users/me",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/users/id/1",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/users/nickname/Edgar32",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/follows/me/followings",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/follows/me/followers",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/presents?page=1",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/presents/me?page=1",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/presents/1",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        },
        {
          "get": {
            "url": "/funds/me?page=1",
            "headers": {
              "access-token": "{{token}}"
            }
          }
        }
      ]
    }
  ]
}
